.box-size i { width: 26px; height: 26px; text-align: center; margin-top: 7px; cursor: pointer; display: inline-block; background: rgb(211, 211, 211); border-radius: 20px; position: relative; opacity: 0.7; }

.box-size i:hover { opacity: 1; }

.box-size i::after, .box-size i::before { content: ""; position: absolute; width: 10px; height: 8px; border: 1px solid rgb(88, 88, 88); left: 8px; top: 10px; background: rgb(255, 255, 255); box-sizing: border-box; }

.box-size i::before { height: 3px; top: 8px; }

.box-size.maximized i::before { height: 4px; top: 11px; }

.box-size.maximized i::after { display: none; }

.social { float: right; margin-top: 4px; display: block; }

.social a { color: rgb(255, 255, 255); font-size: 18px; margin-left: 15px; text-decoration: none; }

.social a:hover { color: rgb(30, 187, 163); }

.ad-label { color: rgb(160, 160, 160); letter-spacing: 0.5px; font-size: 10.4px; text-transform: uppercase; text-align: center; position: absolute; z-index: 9; left: 0px; right: 0px; max-width: 728px; display: block; }

.leaderboard .ad-label { top: -10px; }

.sidebar .ad-label { top: 10px; }

.bottom-ad .ad-label { top: -20px; }

@media screen and (max-width: 800px) {
  .header { height: 46px; padding: 5px 0px; position: fixed; top: 0px; left: 0px; right: 0px; width: 100%; z-index: 99; }
  .site-search, .social { display: none; width: 100%; float: none; position: absolute; top: 45px; margin: 0px; padding: 20px 12px; left: 0px; right: 0px; box-sizing: border-box; z-index: 999; background: rgb(255, 255, 255); }
  .site-search { padding-right: 65px; }
  .site-search .search { margin-left: 0px; box-sizing: border-box; float: left; height: 32px; line-height: 32px; width: 97%; }
  .site-search .search-btn { width: 55px; float: right; height: 32px; margin-right: -55px; box-shadow: none; background: rgb(30, 187, 163); border-radius: 2px; }
  .site-search .search-btn i { font-size: 24px; line-height: normal; }
  .social { text-align: center; }
  .social a { color: rgb(65, 65, 65); margin: 0px 5%; }
  .header button { height: 36px; color: rgb(255, 255, 255); border: none; display: inline-block; background: transparent; padding: 0px 12px; outline: none; cursor: pointer; }
  .header button i { font-size: 29px; line-height: 36px; }
  .open-menu { float: left; margin-right: 7px; position: relative; z-index: 999 !important; }
  .open-search { float: right; }
  .open-share { float: right; }
  .icon-bar { background: rgb(255, 255, 255); display: block; height: 2px; width: 18px; margin: 4px 0px; }
  .leftcolumn { position: fixed; left: 0px; top: 46px; line-height: 20px; bottom: 0px; width: 260px; padding: 0px 6px 0px 12px; overflow: auto; z-index: 9999; background: rgb(255, 255, 255); }
  .link-box, .tool-box, .tools-list { width: 100%; float: none; text-align: center; margin: 0px; }
  .tools-list a, .link-box a { float: none; display: block; margin: 0px; }
  .tools-list a, .tools-list a:nth-child(2n) { width: 100%; }
  .footer-inner { padding-top: 30px; }
  .footer h4::after { right: 0px; margin: 0px auto; }
  .bottom-strip { height: auto; }
  .notice, .social-media { float: none; text-align: center; }
  .social-media { margin: 15px 0px 10px; }
  .footer-logo { display: none; float: none; text-align: center; }
  .footer-logo img { position: static; margin-top: 20px; }
  .appeal { padding: 15px 10px; text-align: center; }
  .header .site-search { width: 100%; float: none; position: absolute; top: 45px; padding: 20px 65px 50px 10px; left: 0px; right: 0px; box-sizing: border-box; z-index: 999; background: rgb(45, 74, 91); border-top: 1px solid rgb(162, 193, 210); }
  .header .site-search label.mobile-only { display: block; text-align: center; color: rgb(255, 255, 255); font-size: 14px; font-weight: 700; padding-bottom: 10px; }
  .codebox-title { position: relative; }
  .codebox-title .code-style { float: none; margin-top: 6px; margin-left: 8px; position: absolute; top: 38px; left: 0px; }
  .social-media-link { margin-top: 0px; line-height: 26px; }
  .social-media-link .appeal-text { text-align: center; }
  .report-error { box-sizing: border-box; width: 100%; border: none; }
  .contact-form p { float: none; }
  .report-error .contact-form p.name, .report-error .contact-form p.email { width: 100%; }
  .contact-form input[type="text"], .contact-form textarea { box-sizing: border-box; width: 100% !important; }
  ul.tree-menu li .shorthand { display: none; }
  ul.tree-menu li ul { margin: 0px 0px 0px 12px; background: none; display: block !important; }
  ul.tree-menu li ul li { padding: 0px; background: none; }
  .open-menu.mobile-only:hover, .open-nav.mobile-only:hover, .open-search-box:hover { background: rgb(45, 74, 91); }
  .code-style { float: none; margin-top: 7px; margin-left: 8px; position: absolute; top: 38px; margin-bottom: 30px; left: 0px; }
  .code-style span { background: rgb(236, 236, 236); }
  .color-box .shadow { overflow: visible; }
  .backdrop { background: rgba(0, 0, 0, 0.5); height: 100%; width: 100%; position: fixed; z-index: 88; top: 0px; bottom: 0px; left: 0px; right: 0px; }
  a.previous-page, a.next-page, .topic { display: none; }
  a.try-btn:hover, a.download-btn:hover { width: 132px; }
  .bottom-ad .ad-label { color: rgb(153, 153, 153); background: rgb(237, 238, 242); width: 336px; top: 0px; margin: 0px auto; padding: 6px 0px; }
  .snippets { display: block; padding: 0px 10px; }
}

@media screen and (max-width: 1280px) {
  .appeal em { display: none; }
}

a.bmc-btn img { height: 33px; margin-top: 20px; opacity: 0.7; }

a.bmc-btn img:hover { opacity: 1; }

ul.faq-list li a { color: rgb(79, 79, 79); }

ul.faq-list li a:hover { color: rgb(34, 34, 34); }
